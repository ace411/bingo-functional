            "src/Algorithms/AddKeys.php",
            "src/Algorithms/Throttle.php",
            "src/Algorithms/Head.php",
            "src/Algorithms/Map.php",
            "src/Algorithms/Last.php",
            "src/Algorithms/Mean.php",
            "src/Algorithms/Filter.php",
            "src/Algorithms/Reject.php",
            "src/Algorithms/Fold.php",
            "src/Algorithms/Unique.php",
            "src/Algorithms/Compact.php",
            "src/Algorithms/Flatten.php",
            "src/Algorithms/DropLeft.php",
            "src/Algorithms/DropRight.php",
            "src/Algorithms/Partition.php",
            "src/Algorithms/IsArrayOf.php",
            "src/Algorithms/ToException.php",
            "src/Algorithms/PartialRight.php",
            "src/Algorithms/ArrayKeysExist.php",
            "src/Algorithms/ConstantFunction.php",
            "src/PatternMatching/functions.php"
