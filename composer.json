    "functional",
    "pattern matching",
    "curry",
    "partial application",
    "helpers",
    "tuples",
    "zip",
    "input-output",
    "unzip",
    "compose",
    "partial",
    "filterM",
    "foldM",
    "lenses",
    "lens",
    "monad laws",
    "applicative laws",
    "composition",
    "persistent data structures",
    "map",
    "reduce",
    "fold",
    "filter",
    "monads",
    "immutability",
    "IO monad",
    "Reader monad",
    "Writer monad",
    "State monad",
    "referential transparency",
    "fp",
    "pure functions",
    "higher-order functions",
    "functional programming",
    "bingo",
    "Either",
    "Maybe",
    "Applicative",
    "Functor",
    "fantasy land",
    "collections",
    "immutable lists",
    "union types",
    "functors",
    "applicatives"
