{
    "name": "chemem/bingo-functional",
    "type": "library",
    "description": "A simple functional programming library.",
    "keywords": [
        "functional",
        "pattern matching",
        "curry",
        "partial application",
        "helpers",
        "zip",
        "map",
        "reduce",
        "fold",
        "filter",
        "monads",
        "immutability",
        "IO monad",
        "Reader monad",
        "Writer monad",
        "State monad",
        "referential transparency",
        "fp",
        "functional programming",
        "bingo",
        "Either",
        "Maybe",
        "Applicative",
        "Functor",
        "fantasy land",
        "collections",
        "immutable lists",
        "union types",
        "functors",
        "applicatives"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lochemem Bruno Michael",
            "email": "lochbm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6",
        "squizlabs/php_codesniffer": "~3"
