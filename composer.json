{
    "name": "chemem/bingo-functional",
    "description": "A simple functional programming library.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lochemem Bruno Michael",
            "email": "lochbm@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "require": {
        "php": ">=7.0.0"
    },
    "suggest": {
        "chemem/bingo-functional-repl": "A simple REPL for the bingo-functional library"
    },
    "autoload": {
        "psr-4": {
            "Chemem\\Bingo\\Functional\\": "src/"
        },
        "files": [
            "src/Algorithms/Identity.php",
            "src/Algorithms/Compose.php",
            "src/Algorithms/Curry.php",
            "src/Algorithms/CurryN.php",
            "src/Algorithms/Pick.php",
            "src/Algorithms/Pluck.php",
            "src/Algorithms/Zip.php",
            "src/Algorithms/Any.php",
            "src/Algorithms/Max.php",
            "src/Algorithms/Min.php",
            "src/Algorithms/Omit.php",
            "src/Algorithms/Every.php",
            "src/Algorithms/Where.php",
            "src/Algorithms/GroupBy.php",
            "src/Algorithms/Fill.php",
            "src/Algorithms/ToPairs.php",
            "src/Algorithms/MapDeep.php",
            "src/Algorithms/FromPairs.php",
            "src/Algorithms/Trampoline.php",
            "src/Algorithms/IndexOf.php",
            "src/Algorithms/Unzip.php",
            "src/Algorithms/PartialLeft.php",
            "src/Algorithms/Extend.php",
            "src/Algorithms/Memoize.php",
            "src/Algorithms/Tail.php",
