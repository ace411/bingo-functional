        "pattern matching",
        "curry",
        "partial application",
        "helpers",
        "zip",
        "map",
        "reduce",
        "fold",
        "filter",
        "monads",
        "immutability",
        "IO monad",
        "Reader monad",
        "Writer monad",
        "State monad",
        "referential transparency",
        "fp",
        "functional programming",
        "bingo",
        "Either",
        "Maybe",
        "Applicative",
        "Functor",
        "fantasy land",
        "collections",
        "immutable lists",
        "union types",
        "functors",
        "applicatives"
    ]
